{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 3 versions"]
      }
    }]
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "transform-object-rest-spread",
    "react-hot-loader/babel",
    ["babel-plugin-styled-components" , { "ssr": true }]
  ]
}