{
  "name": "genesis",
  "version": "1.0.0",
  "description": "My coolest personal website build from scratch using cutting edge full JS stack.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -k -r 'npm run start:server' 'npm run start:webpack'",
    "start:server": "nodemon src/server/server.js --exec babel-node",
    "start:webpack": "webpack --mode development --watch",
    "start:dev": "webpack-dev-server",
    "build": "webpack",
    "heroku": "git push heroku master"
  },
  "keywords": [
    "javascript",
    "react",
    "redux",
    "node",
    "graphql",
    "css-grid",
    "docker",
    "styled-componets",
    "auth2"
  ],
  "author": "Tudor Tacal",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.0",
    "nodemon": "^1.17.5",
    "styled-components": "^3.3.3",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
